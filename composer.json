{
    "name": "presta/composer-public-bundle",
    "description": "A symfony 2 bundle that provides a simple way to include public 3rd-party libraries (javascripts, css, pictures ...) into your projects and keep its up-to-date.",
    "keywords": ["symfony2", "bundle", "public", "javascript", "css", "3rd party", "prestaconcept"],
    "license": "MIT",
    "authors": [
        {
            "name": "David Epely",
            "email": "depely@prestaconcept.net",
            "role": "Initial developer"
        },
        {
            "name": "Nicolas Joubert",
            "email": "njoubert@prestaconcept.net",
            "role": "Maintainer, Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/symfony": ">=2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.22"
    },
    "autoload": {
        "psr-0": { "Presta\\ComposerPublicBundle": "" }
    },
    "target-dir": "Presta/ComposerPublicBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
